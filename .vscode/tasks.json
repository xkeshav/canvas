{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Open HTML in Chrome",
      "type": "shell",
      "command": "google-chrome",
      "args": [
        "${file}"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "never",
        "revealProblems": "onProblem",
        "panel": "new"
      }
    }
  ]
}